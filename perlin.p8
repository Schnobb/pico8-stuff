function _init()
 cls(1)
 poke(0x5f34,1)
 
 f=0
 
 pi=3.14159
 grad={}
 --color_ramp={}
 colors={}
 
 maxx=128
 maxy=128
 
 screenw=32
 screenh=32
 
 xres=flr(128/screenw)
 yres=flr(128/screenh)
 
 y_idx=0
 
 draw=true
 debug=true
 rnd_pat=false
 
 interp_list={
  {f=lerp,name="lerp"},
  {f=smoothstep,name="smoothstep"},
  {f=smootherstep,name="smootherstep"}
 }
 
 interp_idx=#interp_list-1
 interp=nil
 
 col_idx=10
 
 local seed=nil
 local bpat=nil
 
 if rnd_pat then
  bpat=gen_rnd_pat()
 end
 
 init_grad(seed)
 init_colors(64,bpat)
end

function _update60()
 handle_inputs()
 
 interp=interp_list[interp_idx+1].f
 
 f=sin(time())+1
 f/=2
end

function _draw()
 draw_cloud()
 --draw_graph()
 if debug then
  draw_debug()
 end
end

function handle_inputs()
	if btnp(❎) then
  interp_idx=(interp_idx+1)%#interp_list
  draw=true
 end
 
 if btnp(🅾️) then 
  init_grad()
  if rnd_pat then
   init_colors(64,gen_rnd_pat())
  end
  draw=true
 end
 
 if btn(⬆️) then
  y_idx+=1
  draw=true
 elseif btn(⬇️) then
  y_idx=max(0,y_idx-1)
  draw=true
 end
 
 if btnp(⬅️) then
  col_idx=(col_idx-1)%16
  draw=true
 elseif btnp(➡️) then
  col_idx=(col_idx+1)%16
  draw=true
 end
end

function draw_cloud()
 if draw then
  cls()
	 for y=0,screenh do
	 	for x=0,screenw do
	 		
	 		local x0=(x/screenw)*1
	 		local y0=(y/screenh)*1
	 		local v=perlin(x0,y0)
	 		
	 		local x1=(x/screenw)*2
	 		local y1=y+y_idx
	 		
	 		y1=(y1/screenh)*2
	 		v*=perlin(x1,y1)
	 		
	 		local x2=x+y_idx
	 		local y2=(y/screenh)*4
	 		
	 		x2=(x2/screenw)*4
	 		v/=perlin(x2,y2)
	 		
	 		local c=hcolor(col_idx,min(v,1))
	 	 rectfill(x*xres,y*yres,x*xres+xres,y*yres+yres,c)
			end
	 end
	 
	 draw=false
	end
end

function draw_graph()
 cls()
	for x=0,128 do
		local x0,y0=(x/1741)*maxx,y_idx
		local v=perlin(x0,y0)+1
		v/=2
		pset(x,128-flr(v*128),8)
	end
end

function draw_debug()
	rectfill(0,0,128,6,0)
 cursor(0,0)
 color(7)
 print(stat(1))
 local s="col: "..col_idx.." - "
 s=s..interp_list[interp_idx+1].name
 cursor(128-px_len(s),0)
 print(s)
end
